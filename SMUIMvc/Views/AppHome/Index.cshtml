
@{
    ViewBag.Title = "Home";
}

<div class="jumbotron">
    <h1>Home</h1>
    <p class="lead">@ViewBag.Title - @ViewBag.Message</p>
    @*<p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>*@



    <ul class="nav nav-tabs">
        <li class="active">
            <a data-toggle="tab" href="#home">Home</a>
        </li>
        <li>
            <a href="#menu1" data-toggle="tab">Menu1</a>
        </li>
        <li>
            <a href="#menu2" data-toggle="tab">Menu2</a>
        </li>
    </ul>

    <div class="tab-content">
        <div class="tab-pane fade in active" id="home">
            <h3>Home</h3>
            <p>Some Content</p>
        </div>
        <div class="tab-pane fade" id="menu1">
            <h3>Menu1</h3>
            <p>Some Content</p>
        </div>
        <div class="tab-pane fade" id="menu2">
            <h3>Menu2</h3>
            <p>Some Content</p>
        </div>
    </div>
</div>

